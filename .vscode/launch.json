{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch FullTextSearch",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
            "program": "${workspaceFolder}/FullTextSearch/bin/Debug/net8.0/FullTextSearch.dll",
      "args": [],
      "cwd": "${workspaceFolder}/FullTextSearch",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "Now listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:5001"
      }
    }
  ]
}
